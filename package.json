{
  "name": "isomorphic-with-reactjs",
  "version": "1.0.0",
  "description": "This is intended to build a sample serverside rendering applicationusing reactjs technology. ",
  "main": "index.js",
  "engines": {
    "node": "<=6.14.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel src/es6.js --xwatch --out-dir build --source-maps && echo \"\n\n=========output of es6:===========\n\n\" && node build/src/es6.js",
    "start": "NODE_ENV=Developement BABEL_CACHE_PATH=./node_modules/.bin/cache/shahid-cache.json & ./node_modules/.bin/nodemon webpack/server/main.js --exec ./node_modules/.bin/babel-node",
    "dev-harmony": "node --harmony ./src/es6.js",
    "set-environment": "NODE_ENV=Development ",
    "webpack-server": "./node_modules/.bin/nodemon webpack/server/main.js --exec node_modules/.bin/babel-node",
    "dev-server": "./node_modules/.bin/nodemon src/server/main.js --exec node_modules/.bin/babel-node",
    "dev-server-with-harmony": "./node_modules/.bin/nodemon --use-strict --harmony src/server",
    "start-server": "npm-run-all -p set-environment webpack-server dev-server-with-harmony"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shahidql/reactj-isomorphic.git"
  },
  "keywords": [
    "isomorphic",
    "reactjs",
    "nodejs",
    "express",
    "webpack",
    "gulp"
  ],
  "author": "shahid iqbal",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shahidql/reactj-isomorphic/issues"
  },
  "homepage": "https://github.com/shahidql/reactj-isomorphic#readme",
  "devDependencies": {
    "axios": "^0.7.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.26.0",
    "consolidate": "^0.15.1",
    "crypto": "^1.0.1",
    "css-loader": "^0.19.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "getconfig": "^4.5.0",
    "handlebars": "^4.0.11",
    "history": "^1.17.0",
    "memcached": "^2.2.2",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^3.13.1",
    "nodemon": "^1.17.3",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-router": "^2.0.0",
    "sass-loader": "^3.0.0",
    "style-loader": "^0.12.4",
    "webpack": "^4.12.2",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.2"
  }
}
